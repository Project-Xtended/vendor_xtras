	xmlns:tools="http://schemas.android.com/tools" >
	<item >
		<shape>
                <gradient
                    android:angle="45"
                    android:startColor="@*android:color/gradient_end"
                    android:endColor="@*android:color/gradient_start" />
			<corners android:radius="?android:dialogCornerRadius" />
			
		</shape>
	</item>
    <item android:bottom="2.0dip" android:left="2.0dip" android:right="150.0dip" android:top="2.0dip" >
