			<solid android:color="@*android:color/notification_material_background_color" />
			<corners android:radius="?android:dialogCornerRadius" />

		</shape>
	</item>
	<item android:bottom="5dip" android:top="2.0dip" android:left="2.0dip" android:right="2.0dip" >
		<shape>
			<solid android:color="@*android:color/notification_material_background_color" />
			<corners android:radius="?android:dialogCornerRadius" />
			
		</shape>
	</item>
</layer-list>
